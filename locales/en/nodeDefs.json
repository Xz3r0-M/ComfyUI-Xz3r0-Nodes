      "0": {
        "name": "width",
        "tooltip": "Output width"
      },
      "1": {
        "name": "height",
        "tooltip": "Output height"
      }
    }
  },
  "XImageSave": {
    "display_name": "XImageSave",
    "description": "Advanced image saving node with custom filename, subfolder support, and datetime placeholders.",
    "inputs": {
      "images": {
        "name": "Images",
        "tooltip": "Input image tensor (B, H, W, C)"
      },
      "filename_prefix": {
        "name": "Filename Prefix",
        "tooltip": "Filename prefix, supports datetime placeholders: %Y%, %m%, %d%, %H%, %M%, %S%"
      },
      "subfolder": {
        "name": "Subfolder",
        "tooltip": "Subfolder name (no path separators allowed), e.g., Characters or images_%Y%-%m%-%d%"
      },
      "compression_level": {
        "name": "Compression Level",
        "tooltip": "PNG compression level (0=no compression, 9=maximum compression)"
      }
    },
    "outputs": {
      "0": {
        "name": "Images",
        "tooltip": "Input images (passed through)"
      },
      "1": {
        "name": "save_path",
        "tooltip": "Saved file path"
      }
    }
  },
