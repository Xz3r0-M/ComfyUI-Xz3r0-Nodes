{
  "XMath": {
    "display_name": "XMath",
    "description": "Mathematical operation node supporting addition, subtraction, multiplication, division, power, and modulo operations with dual output format.",
    "inputs": {
      "operation": {
        "name": "Operation",
        "tooltip": "Mathematical operation type",
        "options": {
          "Addition (+)": "Addition (+)",
          "Subtraction (-)": "Subtraction (-)",
          "Multiplication (×)": "Multiplication (×)",
          "Division (÷)": "Division (÷)",
          "Power (**)": "Power (**)",
          "Modulo (%)": "Modulo (%)",
          "Maximum": "Maximum",
          "Minimum": "Minimum"
        }
      },
      "a": {
        "name": "Value A",
        "tooltip": "First input value"
      },
      "b": {
        "name": "Value B",
        "tooltip": "Second input value"
      }
    },
    "outputs": {
      "0": {
        "name": "result_int",
        "tooltip": "Integer result (rounded)"
      },
      "1": {
        "name": "result_float",
        "tooltip": "Floating point result"
      }
    }
  },
  "XResolution": {
    "display_name": "XResolution",
